        function setValueBadge(target, rs) {
            var c = target.find('.badge');
            if (c != undefined && c.length > 0) {
                c.remove();
            }
            if (rs != undefined && rs > 0) {
                var html = "<span class=\"badge badge-danger\"> " + rs + " <span>";
                target.append(html);
            }

        }
        function countThongBao() {
            var urlCount = '@Url.Action("BadgeThongBao", "Notification")';
            $.ajax({
                url: urlCount,
                success: function (e) {
                    setValueBadge($('.badge_thongbao'), e.Count);
                }
            });
        }
        function countXuLyVanBanDen() {
